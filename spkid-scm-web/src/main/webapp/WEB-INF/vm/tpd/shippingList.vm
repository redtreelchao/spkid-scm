<div id="listDiv">
	<table id="dataTable" class="dataTable" width="100%" cellpadding="0" cellspacing="0">
		<tbody>
			<tr>
				<th width="180px;">物流公司名称</th>
				<th width="180px;">物流公司代码</th>
				<th >物流公司描述</th>
			</tr>
			#foreach( $info in $shippingList) 
			<tr>
				<td align="center"> $!info.shippingName </td>
				<td align="center"> $!info.shippingCode </td>
				<td style="padding-left:10px;"> $!info.shippingDesc </td>
			</tr>
			#end
		</tbody>
	</table>
</div>

<div class="clear"></div>
